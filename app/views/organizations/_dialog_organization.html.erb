<div class="row clickable" onclick="<%=
    if @alias.blank?
      link_to_add_workday_organization_fields(organization, session[:workday_id])
    else
      link_to_select(organization, @alias)
    end %>" >
  <li>
    <div class="col-md-3 col-md-offset-1">
      <%= organization.name %>
    </div>
    <div class="col-md-2 col-md-offset-1">
      <%= organization.contact_name %>
    </div>
    <div class="col-md-3">
      <%= organization.city %>
    </div>
  </li>
</div>
