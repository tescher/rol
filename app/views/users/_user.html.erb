<div class="row clickable" href="<%=edit_user_path(user)%>">
  <li>
    <div class="col-md-2 col-md-offset-1">
      <%= user.name %>
    </div>
    <div class="col-md-2">
      <%= user.email %>
    </div>
    <div class="col-md-2">
      <%= user.admin? ? "Admin" : "" %>
    </div>
  </li>
</div>
