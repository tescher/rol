<% provide(:title, 'Organization Types') %>

<h1>All Organization Types</h1>

<% if @organization_types.count > 0 %>
    <ul class="listing">
      <%= render @organization_types %>
    </ul>
<% else %>
    No organization types found. Click New to create one.
<% end %>

<%= will_paginate %>

<div class="actions pull-right">
    <%= link_to 'New Organization Type', new_organization_type_path, class: "btn btn-primary" %>
    <%= link_to 'Back', root_path, class: "btn btn-primary" %>
</div>