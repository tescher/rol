<% provide(:title, 'Contact Types') %>

<h1>All Contact Types</h1>

<% if @contact_types.count > 0 %>
    <ul class="listing">
      <%= render @contact_types %>
    </ul>
<% else %>
    No contact types found. Click New to create one.
<% end %>

<%= will_paginate %>

<div class="actions pull-right">
    <%= link_to 'New Contact Type', new_contact_type_path, class: "btn btn-primary" %>
    <%= link_to 'Back', root_path, class: "btn btn-primary" %>
</div>