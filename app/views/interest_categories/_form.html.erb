<%= form_for(@interest_category) do |f| %>
  <%= @object = @interest_category
      render 'shared/error_messages' %>

  <div class="field">
    <%= f.label :name %><br>
    <%= f.text_field :name, class: 'form-control' %>
  </div>
  <div class="actions">
    <%= f.submit class: "btn btn-primary" %>
  </div>
<% end %>
