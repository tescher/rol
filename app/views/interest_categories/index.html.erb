<% provide(:title, 'Interest Categories') %>

<h1>All Interest Categories</h1>

<% if @interest_categories.count > 0 %>
    <ul class="listing">
      <%= render @interest_categories %>
    </ul>
<% else %>
    No interest categories found. Click New to create one.
<% end %>

<%= will_paginate %>

<div class="actions">
    <%= link_to 'New Interest Category', new_interest_category_path, class: "btn btn-primary" %>
    <%= link_to 'Back', root_path, class: "btn btn-primary" %>
</div>